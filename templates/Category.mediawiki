<hr />

{{#if:{{{has_categories|}}}|
<h2>Subcategories</h2>
<ul class="category-subcategories">
    {{categories}}
</ul>
}}

{{#if:{{{has_pages|}}}|
<h2>Pages</h2>
<ul class="category-pages">
    {{pages}}
</ul>
}}

{{#if:{{{has_templates|}}}|
<h2>Templates</h2>
<ul class="category-templates">
    {{templates}}
</ul>
}}

{{#if:{{{has_files|}}}|
<h2>Files</h2>
<ul class="category-files gallery">
    {{files}}
</ul>
}}

{{#if:{{{has_other_language|}}}|
<h2>Entries that don't belong to the language of this category</h2>
<ul class="category-other-languages">
    {{other_language}}
</ul>
}}
